    <Color x:Key="WindowBackgroundAnimationColorPrimary">#ffffff</Color>
    <Color x:Key="WindowBackgroundAnimationColorSecondary">#ead7ed</Color>
    <SolidColorBrush x:Key="ControlsBackgroundColor" Color="#2c2f33"/>
    <SolidColorBrush x:Key="ButtonHoverColor" Color="#F3EEEA"/>
    <SolidColorBrush x:Key="ButtonDisabledColor" Color="#C4B6B6"/>
    <SolidColorBrush x:Key="ButtonCheckedColor" Color="#94F3E4"/>
    <SolidColorBrush x:Key="ButtonCheckedHoverColor" Color="#37AA9C"/>
    <SolidColorBrush x:Key="TextColorSecondary" Color="#23272a"/>
    <SolidColorBrush x:Key="TextColorPrimary" Color="#ffffff"/>
